{
  "name": "googleapis-nodejs-functions",
  "description": "Google Cloud Functions Client Library for Node.js (unofficial)",
  "version": "0.0.3",
  "license": "Apache-2.0",
  "author": "vizzuality-research",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": "vizzuality/googleapis-nodejs-functions",
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "files": [
    "build/src",
    "LICENSE"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google cloud functions",
    "cloud functions"
  ],
  "contributors": [
    "Sergio Gordillo <sgordillogallardo@gmail.com>"
  ],
  "scripts": {
    "lint": "eslint samples/",
    "prettier": "prettier --write samples/*.js samples/*/*.js",
    "cover": "nyc --reporter=lcov mocha test/*.js && nyc report",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "test": "mocha build/test",
    "system-test": "mocha build/system-test --timeout 600000",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "compile-samples": "tsc -p tsconfig-samples.json",
    "js-samples": "NODE_PATH=./samples node ./samples/functions.js",
    "ts-samples": "NODE_PATH=./samples-ts node ./samples-ts/functions.js",
    "fix": "gts fix"
  },
  "dependencies": {
    "@google-cloud/common": "^0.24.0",
    "@google-cloud/paginator": "^0.1.0",
    "@google-cloud/promisify": "^0.3.0",
    "arrify": "^1.0.0",
    "extend": "^3.0.0",
    "is": "^3.0.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/arrify": "^1.0.4",
    "@types/extend": "^3.0.0",
    "@types/is": "0.0.20",
    "@types/mocha": "^5.2.3",
    "@types/request": "^2.47.1",
    "@types/tmp": "0.0.33",
    "codecov": "^3.0.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^3.0.0",
    "eslint-plugin-node": "^7.0.0",
    "eslint-plugin-prettier": "^2.3.1",
    "gts": "^0.8.0",
    "mocha": "^5.0.0",
    "node-fetch": "^2.2.0",
    "normalize-newline": "^3.0.0",
    "nyc": "^13.0.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.0",
    "prop-assign": "^1.0.0",
    "proxyquire": "^2.1.0",
    "source-map-support": "^0.5.6",
    "tmp": "^0.0.33",
    "tslint": "5.3.2",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^3.0.1",
    "uuid": "^3.1.0"
  }
}
